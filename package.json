{
  "name": "mocha-automatic-coffeemaker",
  "version": "0.0.1",
  "description": "Generate a test code of mocha by the same path structure",
  "main": "index.js",
  "scripts": {
    "manual-test": "./bin/genmochatest -f -o manual-test/foo -p manual-test-dist -t manual-test/template.js manual-test/foo/bar/b.js && cat manual-test-dist/bar/b.js",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjirou/mocha-automatic-coffeemaker.git"
  },
  "keywords": [
    "mocha",
    "generator",
    "test"
  ],
  "author": "kjirou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kjirou/mocha-automatic-coffeemaker/issues"
  },
  "homepage": "https://github.com/kjirou/mocha-automatic-coffeemaker#readme",
  "engines": {
    "node": ">=0.12",
    "npm": ">=2"
  },
  "bin": {
    "genmochatest": "./bin/genmochatest"
  },
  "files": [
    "LICENSE",
    "README.md",
    "bin",
    "index.js",
    "lib",
    "manual-test",
    "package.json",
    "test"
  ],
  "dependencies": {
    "escape-string-regexp": "1.0.3",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "promise-defer": "1.0.0"
  },
  "devDependencies": {
    "mocha": "2.3.3"
  }
}
